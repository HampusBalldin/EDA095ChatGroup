<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="Users" type="Users" />
	<xsd:complexType name="Users">
		<xsd:sequence>
			<xsd:element name="User" type="User" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="User">
		<xsd:sequence>
			<xsd:element name="uid" type="xsd:string" />
			<xsd:element name="pwd" type="xsd:string" />
			<xsd:element name="status" type="Status" />
		</xsd:sequence>
	</xsd:complexType>


</xsd:schema>