<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="Friends" type="Friends" />
	<xsd:complexType name="Friends">
		<xsd:sequence>
			<xsd:element name="Friend" type="Friend" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="Friend">
		<xsd:sequence>
			<xsd:element name="uid_1" type="xsd:string" />
			<xsd:element name="uid_2" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>